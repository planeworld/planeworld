    hook="sun"
    enable_angle_hook="true"
    position_x="10.0"
    position_y=".0"
    viewport_width="600.0"
    viewport_height="400.0"
  />

  <config file="config.xml"/>
  
  <object file="sun.xml"/> 
  <object file="mercury.xml"/>
  <object file="venus.xml"/>
  <object file="earth.xml"/>
  <object file="moon.xml"/>
  <object file="mars.xml"/>
  <object file="jupiter.xml"/>
  <object file="saturn.xml"/>
  <object file="uranus.xml"/>
  <object file="neptune.xml"/>
</universe>
