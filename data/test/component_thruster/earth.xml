<!DOCTYPE PlaneworldML>
<!-- Create planet earth -->
<object type="rigidbody">
    <core
        name="Earth"
        origin_x="0.0"
        origin_y="0.0"
        dynamics="true"
        gravity="true"
        velocity_x="29780.0"
    />
    <body_core
        angle_velocity__deg="0.0041780742"
    />
    <shape
        type="shape_planet"
        mass="5.974e24"
        radius="6378.16e3"
        height_max="8000.0"
        sea_level="-0.2"
        ground_resolution="0.001"
        center_x="0.0"
        center_y="0.0">
    </shape>
    <!-- Create Launch Pad on Earth -->
    <shape
        type="shape_polygon"
        polygon_type="line_strip"
        mass="100.0"
        points="-1000.0,  6378.16e3;
                 1000.0,  6378.16e3">
    </shape>
    <shape
        type="shape_polygon"
        polygon_type="line_strip"
        mass="1000.0"
        points="-500.0,  6378.16e3;
                -100.0,  6378.18e3;
                 -10.0,  6378.165e3;
                  10.0,  6378.165e3;
                 100.0,  6378.18e3;
                 500.0,  6378.16e3">
    </shape>
</object>
