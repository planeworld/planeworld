<!DOCTYPE PlaneworldML>
<universe procedural_generation="false">
  <gravity
    vec_x="0.0"
    vec_y="0.0"
  />
  <!-- Create the active camera -->
  <camera
    hook="Mass"
    enable_angle_hook="true"
    angle="0.0"
    position_x="0.0"
    position_y="0.0"
    m_per_px="0.1"
  />
  <config file="config.xml"/>
  <object type="rigidbody">
    <core
        name="Mass"
        mass="1.0"
        origin_x="0.0"
        origin_y="0.0"
        dynamics="true"
        gravity="true"
        velocity_x="0.0"
        velocity_y="0.0"
    />
    <body_core
        angle_velocity="0.0"
        inertia="10.0"
    />
    <shape
        type="shape_circle"
        radius="1.0"
        center_x="0.0"
        center_y="0.0">
        <visuals type="shape_visuals_circle"/>
    </shape>
  </object>
  <object type="rigidbody">
    <core
        name="ReferenceMass"
        mass="1.0"
        origin_x="10.0"
        origin_y="0.0"
        dynamics="true"
        gravity="true"
        velocity_x="0.0"
        velocity_y="0.0"
    />
    <body_core
        angle_velocity="0.0"
        inertia="10.0"
    />
    <shape
        type="shape_circle"
        radius="1.0"
        center_x="0.0"
        center_y="0.0">
        <visuals type="shape_visuals_circle"/>
    </shape>
  </object>  
</universe>
